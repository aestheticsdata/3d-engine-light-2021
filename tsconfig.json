{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@primitives/*": ["src/primitives/*"],
      "@data/*": ["src/data/*"],
      "@textures/*": ["src/textures/*"],
    },
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "target": "ESNext"
  }
}
